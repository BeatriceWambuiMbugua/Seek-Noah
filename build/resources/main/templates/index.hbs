{{#partial "content"}}
    <div class="data-image"></div>
    <div class="container" id="data">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    {{#if endangeredSightings}}
                        <table class="table table-sm table-dark">
                            <thead>
                            <tr>
                                <th scope="col">Type</th>
                                <th scope="col">Name</th>
                                <th scope="col">Age</th>
                                <th scope="col">Health</th>
                                <th scope="col">Location</th>
                                <th scope="col">Ranger</th>
                            </tr>
                            </thead>
                            {{#each endangeredSightings}}
                                <tbody>
                                <tr>
                                    <th >{{type}}</th>
                                    <td>{{speciesName}}</td>
                                    <td>{{speciesAge}}</td>
                                    <td>{{speciesHealth}}</td>
                                    <td>{{locationId}}</td>
                                    <td>{{rangerId}}</td>
                                </tr>
                                </tbody>
                            {{/each}}
                        </table>
                    {{/if}}
                </div>
                <div class="col-md-6">
                    {{#if normalSightings}}
                        <table class="table table-sm table-dark">
                            <thead>
                            <tr>
                                <th scope="col">Type</th>
                                <th scope="col">Name</th>
                                <th scope="col">Location</th>
                                <th scope="col">Ranger</th>
                            </tr>
                            </thead>
                            {{#each normalSightings}}
                                <tbody>
                                <tr>
                                    <th >{{type}}</th>
                                    <td>{{speciesName}}</td>
                                    <td>{{locationId}}</td>
                                    <td>{{rangerId}}</td>
                                </tr>
                                </tbody>
                            {{/each}}
                        </table>
                        </div>
                    {{/if}}
                </div>
            </div>
        </div>

{{/partial}}

